class HashTable:
    def __init__(self,size):
        self.size=size
        self.table=[None]*size

    def hash_func(self,key):
        return key%self.size

    def insert(self,key):
        i=self.hash_func(key)
        start=i
        while self.table[i] is not None and self.table[i]!=-1:
            i=(i+1)%self.size
            if i==start:
                print("Table Full")
                return
        self.table[i]=key
        print("Inserted")

    def search(self,key):
        i=self.hash_func(key)
        start=i
        while self.table[i] is not None:
            if self.table[i]==key:
                print("Key Found at index",i)
                return
            i=(i+1)%self.size
            if i==start:
                break
        print("Key Not Found")

    def delete(self,key):
        i=self.hash_func(key)
        start=i
        while self.table[i] is not None:
            if self.table[i]==key:
                self.table[i]=-1
                print("Deleted")
                return
            i=(i+1)%self.size
            if i==start:
                break
        print("Key Not Found")

    def display(self):
        for i in range(self.size):
            print(i,":",self.table[i])

h=HashTable(10)
while True:
    print("\n1.Insert 2.Search 3.Delete 4.Display 5.Exit")
    ch=int(input())
    if ch==1:
        h.insert(int(input("Enter Key: ")))
    elif ch==2:
        h.search(int(input("Enter Key: ")))
    elif ch==3:
        h.delete(int(input("Enter Key: ")))
    elif ch==4:
        h.display()
    elif ch==5:
        break
