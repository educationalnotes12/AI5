import heapq

class Graph:
    def __init__(self,n):
        self.n=n
        self.graph={chr(65+i):{} for i in range(n)}

    def add_edge(self,u,v,w):
        self.graph[u][v]=w
        self.graph[v][u]=w

    def dijkstra(self,start):
        dist={x:float('inf') for x in self.graph}
        dist[start]=0
        pq=[(0,start)]
        while pq:
            d,u=heapq.heappop(pq)
            for v,w in self.graph[u].items():
                if dist[v]>d+w:
                    dist[v]=d+w
                    heapq.heappush(pq,(dist[v],v))
        print("Minimum time from",start,":")
        for k in dist:
            print(k,"=",dist[k])

n=int(input("Enter number of locations: "))
g=Graph(n)
e=int(input("Enter number of routes: "))
for _ in range(e):
    u,v,w=input("Enter route and time (e.g. A B 5): ").split()
    g.add_edge(u,v,int(w))

start=input("Enter starting location: ")
g.dijkstra(start)
